    "eslint-plugin-react": "7.16.0",
    "gatsby-cli": "2.7.54",
    "jest": "24.9.0",
    "mermaid.cli": "0.5.1",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "stylelint": "11.0.0",
    "stylelint-config-recommended": "3.0.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
