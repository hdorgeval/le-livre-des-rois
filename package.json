    "gatsby-source-filesystem": "2.1.29",
    "gatsby-transformer-remark": "2.6.27",
    "node-sass": "4.12.0",
    "randomcolor": "0.5.4",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-helmet": "5.2.1",
    "unist-util-visit": "2.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/mermaid": "8.2.0",
    "@types/node": "12.7.11",
    "@types/node-sass": "4.11.0",
    "@types/randomcolor": "0.5.3",
